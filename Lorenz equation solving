function dx = lorenz(t,x)
global r s b
s=10;
b=8/3;
r=28;
dx=zeros(3,1);
dx(1)=(s*x(2))-(s*x(1));
dx(2)=r*x(1)-x(2)-(x(1)*x(3));
dx(3)=(x(1)*x(2))-(b*x(3));
****************************************************************************
g = inline('[ 10*(x(2)-x(1)) ; 28*x(1)-x(2)-x(1)*x(3) ; x(1)*x(2)-(8/3)*x(3) ]', 't','x');
 [t,x] = ode45(g,[0,100],[1;2;3]);
 plot3(x(:,1),x(:,2),x(:,3));
